{
  "openapi": "3.0.1",
  "info": {
    "title": "UsuarioApi",
    "version": "1.0"
  },
  "paths": {
    "/Usuario/cadastro": {
      "post": {
        "tags": [
          "Usuario"
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/CreateUsuarioDto"
              }
            },
            "text/json": {
              "schema": {
                "$ref": "#/components/schemas/CreateUsuarioDto"
              }
            },
            "application/*+json": {
              "schema": {
                "$ref": "#/components/schemas/CreateUsuarioDto"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Success"
          }
        }
      }
    },
    "/Usuario/login": {
      "post": {
        "tags": [
          "Usuario"
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/LoginUsuarioDto"
              }
            },
            "text/json": {
              "schema": {
                "$ref": "#/components/schemas/LoginUsuarioDto"
              }
            },
            "application/*+json": {
              "schema": {
                "$ref": "#/components/schemas/LoginUsuarioDto"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Success"
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "CreateUsuarioDto": {
        "required": [
          "dataNascimento",
          "password",
          "rePassword",
          "username"
        ],
        "type": "object",
        "properties": {
          "username": {
            "type": "string"
          },
          "dataNascimento": {
            "type": "string",
            "format": "date-time"
          },
          "password": {
            "type": "string",
            "format": "password"
          },
          "rePassword": {
            "type": "string"
          }
        },
        "additionalProperties": false
      },
      "LoginUsuarioDto": {
        "required": [
          "password",
          "username"
        ],
        "type": "object",
        "properties": {
          "username": {
            "type": "string"
          },
          "password": {
            "type": "string"
          }
        },
        "additionalProperties": false
      }
    }
  }
}